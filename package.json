{
  "name": "@toolkit-f/snowflake-id",
  "version": "1.0.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "Unique ID Generator For NodeJS",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:bm": "vitest run tests/benchmark.test.ts",
    "test:co": "vitest run tests/collision.test.ts",
    "test:ut": "vitest run tests/utils.test.ts",
    "test:watch": "vitest --watch",
    "test:dist:js": "node tests/dist.js",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Toolkit-F/Snowflake-Id.git"
  },
  "keywords": [
    "snowflake",
    "id",
    "uuid",
    "unique-id",
    "distributed",
    "typescript",
    "generator"
  ],
  "author": {
    "name": "Toolkit-F",
    "url": "https://github.com/Toolkit-F"
  },
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Toolkit-F/Snowflake-Id/issues"
  },
  "homepage": "https://github.com/Toolkit-F/Snowflake-Id#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.17"
  }
}